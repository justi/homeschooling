= render 'users/profile'
%h1=@tag.name
%h3
  =@users.count
  Ruby on Rails
  =@users.count == 1 ? "person" : "people"
  mention this skill

=gmaps4rails(@json)
%br/
=will_paginate @users
- @users.each do |user|
  .row.span4
    = link_to user.username, user_path(user)
    - if user.country.present?
      %br
      = image_tag("flags/#{user.country.iso.downcase}.png")
      = user.country.printable_name
